{
  "name": "FittedCloset",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "android": "cd bin && ./set-env-dev.sh && cd .. && npx react-native run-android",
    "ios": "cd bin && ./set-env-dev.sh && cd .. && npx react-native run-ios",
    "android:staging": "cd bin && ./set-env-staging.sh && cd .. && npx react-native run-android",
    "ios:staging": "cd bin && ./set-env-staging.sh && cd .. && npx react-native run-ios",
    "android:prod": "cd bin && ./set-env-prod.sh && cd .. && npx react-native run-android",
    "ios:prod": "cd bin && ./set-env-prod.sh && cd .. && npx react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --ignore-pattern 'ios/**' --ignore-pattern 'android/**' --fix",
    "test": "jest",
    "format": "prettier --write \"**/*.js\" --ignore-path .gitignore --ignore-pattern 'ios/**' --ignore-pattern 'android/**'"
  },
  "dependencies": {
